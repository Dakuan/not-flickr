{
  "parser": "babel-eslint",
  "ecmaFeatures": {
    "jsx": true,
    "modules": true
  },
  "env": {
    "es6": true,
    "node": true,
    "browser": true
  },
  "globals": {
    "__DEV__": true,
    "describe": true,
    "it": true
  },
  "rules" : {
     "no-underscore-dangle": 0
  },
  "plugins": [
    "react"
  ]
}
